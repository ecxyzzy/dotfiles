{
    "workbench.colorTheme": "Solarized Dark",
    "workbench.startupEditor": "none",
    "latex-workshop.view.pdf.color.dark.backgroundColor": "#000000",
    "latex-workshop.view.pdf.color.dark.pageColorsBackground": "#002b36",
    "latex-workshop.view.pdf.color.dark.pageColorsForeground": "#fdf6e3",
    "editor.fontFamily": "'SauceCodePro Nerd Font Mono', monospace",
    "latex-workshop.intellisense.atSuggestion.user": {
        "@O": "\\mathcal{O}"
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-bibtex",
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "search.followSymlinks": false,
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "haskell.manageHLS": "GHCup",
    "material-icon-theme.activeIconPack": "none",
    "workbench.iconTheme": "material-icon-theme",
}